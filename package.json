{
    "name": "@wyxos/listing",
    "version": "1.0.0",
    "description": "A Vue 3 composable for managing paginated, filterable data listings with URL synchronization",
    "type": "module",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./src/styles/variables.css": "./src/styles/variables.css"
    },
    "files": [
        "src",
        "tests",
        "README.md",
        "styles"
    ],
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "npm run type-check && npm run test:run",
        "release": "npx @wyxos/zephyr --type=node"
    },
    "keywords": [
        "vue",
        "vue3",
        "listing",
        "pagination",
        "filtering",
        "data-table",
        "axios"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/wyxos/listing.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "vue": "^3.5.0",
        "axios": "^1.11.0",
        "lucide-vue-next": "^0.554.0"
    },
    "peerDependenciesMeta": {
        "@oruga-ui/oruga-next": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@vue/test-utils": "^2.4.6",
        "typescript": "^5.9.3",
        "vitest": "^4.0.13",
        "vue": "^3.5.24",
        "axios": "^1.11.0"
    }
}